{
  "usesTypeScript": false,
  "usesCssModule": false,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/default/TemplateName.jsx",
        "style": "templates/default/TemplateName.scss"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": false
    },
    "page": {
      "customTemplates": {
        "component": "templates/default/TemplateName.jsx",
        "style": "templates/default/TemplateName.scss"
      },
      "path": "src/pages",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": true
    },
    "hook": {
      "customTemplates": {
        "hook": "templates/default/useTemplateName.jsx"
      },
      "path": "src/hooks",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "withHook": true
    }
  },
  "usesStyledComponents": false
}